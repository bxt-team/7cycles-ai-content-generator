{
  "name": "aicos",
  "version": "1.0.0",
  "description": "AICOS - AI-powered content generation system with React frontend",
  "main": "index.js",
  "scripts": {
    "install-frontend": "cd frontend && npm install",
    "dev-frontend": "cd frontend && npm start",
    "build-frontend": "cd frontend && npm run build",
    "dev-backend": "cd backend && uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "setup": "pip install -r requirements.txt && npm run install-frontend",
    "install-website": "cd website && npm install",
    "dev-website": "cd website && npm run dev",
    "build-website": "cd website && npm run build"
  },
  "keywords": ["aicos", "crewai", "content-generation", "ai", "automation"],
